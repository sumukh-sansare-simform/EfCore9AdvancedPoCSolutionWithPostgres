<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="9.0.0" />
		<!-- Fixed package references -->
		<!-- EntityFrameworkCore.DataEncryption is a community package -->
		<PackageReference Include="EntityFrameworkCore.DataEncryption" Version="5.0.0" />
		<!-- Microsoft.OpenApi doesn't exist as a standalone package - it's part of Swashbuckle -->
